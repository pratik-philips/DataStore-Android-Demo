<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <Button
        android:id="@+id/start_preference_storage"
        android:layout_width="150dp"
        android:layout_height="150dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@id/start_proto_data_store"
        android:layout_margin="20dp"
        android:text="Start Data Store Preference"
        tools:ignore="HardcodedText"/>

    <Button
        android:id="@+id/start_proto_data_store"
        android:layout_width="150dp"
        android:layout_height="150dp"
        app:layout_constraintLeft_toRightOf="@+id/start_preference_storage"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_margin="20dp"
        android:text="Start Proto Data Store"
        tools:ignore="HardcodedText"/>

</androidx.constraintlayout.widget.ConstraintLayout>